import math
def solve(a,b):
 n=10001
 seive=[1]*n
 seive[0]=0
 seive[1]=0
 m=int(math.sqrt(n))
 for i in range(2,m+1):
    if seive[i]==1:
        for j in range(i*i,n,i):
            seive[j]=0
c=0
for i in range(a,b+1):
    if seive[i]==1 and seive[i+2] and b+i>=2:
        c=c+1
return c
a,b=(map(int,input().split()))
print(solve(a,b))
